<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:columnCount="7"
    >

    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_cancel_black_24dp"
        android:tag="Cancel"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_check_black_24dp"
        android:tag="Check"
        />

    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_fullscreen_black_24dp"
        android:tag="FullScreen"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_menu_black_24dp"
        android:tag="Menu"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_more_horiz_black_24dp"
        android:tag="More_Horizontal"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_more_vert_black_24dp"
        android:tag="More_Vertical"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_my_location_black_24dp"
        android:tag="MyLocation"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_pan_tool_black_24dp"
        android:tag="Pan"
        android:padding="10dp"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_select_black_24dp"
        android:tag="Select"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_undo_black_24dp"
        android:tag="Undo"
        />

    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_redo_black_24dp"
        android:tag="Redo"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_done_black_24dp"
        android:tag="Done"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_clear_black_24dp"
        android:tag="clear"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_delete_forever_black_24dp"
        android:tag="Delete"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_refresh_black_24dp"
        android:tag="Refresh"
        />

    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_touch_app_black_24dp"
        android:tag="TouchApp"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_unfold_less_black_24dp"
        android:tag="UnfoldLess"
        />
    <ImageView
        android:layout_width="192px"
        android:layout_height="192px"
        android:src="@drawable/ic_unfold_more_black_24dp"
        android:tag="UnfoldMore"
        />
</GridLayout>